{
  "name": "gnosis-safe",
  "version": "0.1.0",
  "description": "Ethereum multisig contract",
  "license": "GPL-3.0",
  "main": "index.js",
  "files": [
    "contracts",
    "test",
    "zos.json",
    "zos.*.json"
  ],
  "scripts": {
    "test-norpc": "node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle test",
    "test": "run-with-testrpc -l 20000000 --noVMErrorsOnRPCResponse true 'truffle test'",
    "deploy-rinkeby": "node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle migrate --reset --network kovan",
    "console-rinkeby": "node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle console --network rinkeby",
    "init-contracts": "node $NODE_DEBUG_OPTION ./node_modules/.bin/truffle exec scripts/init_contracts.js --network mainnet",
    "bulk-watcher": "nodemon --exec babel-node bulk_watcher.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gnosis/gnosis-safe.git"
  },
  "keywords": [
    "Ethereum",
    "Wallet",
    "Safe"
  ],
  "author": "stefan@gnosis.pm",
  "bugs": {
    "url": "https://github.com/gnosis/gnosis-safe/issues"
  },
  "devDependencies": {
    "@digix/tempo": "^0.2.0",
    "@gnosis.pm/mock-contract": "github:gnosis/mock-contract#sol_0_5_0",
    "bignumber.js": "github:frozeman/bignumber.js-nolookahead",
    "eth-lightwallet": "^3.0.1",
    "ethereumjs-abi": "^0.6.5",
    "random-buffer": "*",
    "run-with-testrpc": "^0.3.0",
    "solidity-coverage": "^0.5.11"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "babel-cli": "^6.26.0",
    "bitcore-mnemonic": "^1.5.0",
    "express": "^4.16.4",
    "ganache-cli": "7.0.0-beta.0",
    "global": "^4.3.2",
    "nodemon": "^1.18.10",
    "openzeppelin-solidity": "^2.1.2",
    "solc": "^0.5.0",
    "truffle": "^5.0.1",
    "truffle-hdwallet-provider": "^1.0.2",
    "webpack": "^4.28.4"
  }
}
